[user]
  name = Will Usher
  email = willusher+github@gmail.com
[color]
  ui = always
[alias]
  b = branch
  f = fetch --all
  st = status --short
  co = checkout --
  di = diff
  dc = diff --cached
  cached = diff --cached
  amend = commit --amend
  patch = add -p
  head = !git l -1
  last = show head
  undo = reset --soft HEAD~1
  unstage  = reset ;
  us  = reset ;

  l = log --pretty=oneline --abbrev-commit --graph --decorate
  la = log --pretty=oneline --abbrev-commit --graph --decorate  --all
  lad = log --pretty=oneline --abbrev-commit --graph --decorate  --all  --date=relative

  h = !git head

[core]
  quotepath = false
  editor = '/usr/bin/vim'




