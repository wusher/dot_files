[user]
	name = Will Usher
	email = willusher+github@gmail.com

  
[color]
  ui = always 
[alias]
    st = status
    ci = commit -a 
    co = checkout
    di = diff
    dc = diff --cached
    amend = commit --amend
    aa = add --all
    head = !git l -1
    h = !git head
    r = !git --no-pager l -20
    ra = !git r --all
    ff = merge --ff-only
    pullff = pull --ff-only
    noff = merge --no-ff
    l = log --pretty=oneline --abbrev-commit --graph --decorate 
    lol = log --graph --abbrev-commit --date=relative --oneline --decorate
    
    la = !git l --all
    div = divergence
    gn = goodness
    gnc = goodness --cached
    fa = fetch --all
    pom = push origin master
    b = branch
[core]
	quotepath = false
  editor = '/usr/bin/vim'



  
