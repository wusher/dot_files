[user]
  name = Will Usher
  email = willusher+github@gmail.com
[color]
  ui = always 
[alias]
  co = checkout
  b = branch
  f = fetch --all
  di = diff
  dc = diff --cached
  cached = diff --cached 
  amend = commit --amend
  patch = add -p 
  head = !git l -1

  h = !git head
  l = log --pretty=oneline --abbrev-commit --graph --decorate 
  lol = log --graph --abbrev-commit --date=relative --oneline --decorate
  unstage  = reset 
[core]
  quotepath = false
  editor = '/usr/bin/vim'



  
